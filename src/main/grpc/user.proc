syntax = "proto3";

option java_package = "com.mustafa.payment.grpc.sample";
option java_multiple_files = true;

service User {
    rpc queryUser (Request) returns (Response);
}

message Request {
    uint32 uid = 1;
}

message UserData {
  string username = 1;
  string password = 2;
}

message Response {
  int32 status = 1;
  string message = 2;
  UserData data = 3;
}